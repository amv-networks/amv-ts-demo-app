    "url": "https://www.amv-networks.com"
  },
  "private": true,
  "scripts": {
    "start": "ng serve --open",
    "eject": "ng eject",
    "http-server": "http-server dist/ -c-1",
    "http-server-dev": "http-server dist-dev/ -c-1",
    "build": "ng build --prod --aot --output-path ./dist/",
    "build-dev": "ng build --aot --output-path ./dist-dev/",
    "build-gh": "npm run ci && ng build --prod --aot --output-path ./docs/",
    "build-all": "npm run build && npm run build-dev && npm run build-gh",
    "test": "ng test",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ci": "npm run lint && npm run test && npm run e2e",
    "sme": "ng build && source-map-explorer dist/main.js",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin master",
    "release:major": "standard-version --release-as major && git push --follow-tags origin master",
    "docker": "docker build -t amv-ts-demo . && docker run -d -p 4200:80 amv-ts-demo",
    "postinstall": "node patch.js"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.0.0"
  },
